{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "target": "es5",
    "module": "ESNext",
    "jsx": "react",
    "allowJs": true,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "containers/*": ["src/app/containers/*"],
      "components/*": ["src/app/components/*"],
      "store": ["src/app/store"],
      "store/*": ["src/app/store/*"],
      "hooks": ["src/app/hooks"],
      "hooks/*": ["src/app/hooks/*"],
      "actions/*": ["src/app/actions/*"],
      "layouts/*": ["src/app/layouts/*"],
      "assets/*": ["src/app/assets/*"]
    }
  }
}
